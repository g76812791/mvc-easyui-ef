@using KbaseWeb.Models;
@model System.Collections.Generic.List<caseview>
<div class="productcenter">
    <div class="maincontent01"></div>
    <div class="jdcon">

        @foreach (var d in Model)
        {
            <h2>@d.type.Name</h2>
            <ul>
                @foreach (var m in d.detalis)
                {
                    <li>&middot;@Html.ActionLink(m.Title, "ShowCases", new { Id = m.Id }) </li>
                }
            </ul>
        }
    </div>
</div>
